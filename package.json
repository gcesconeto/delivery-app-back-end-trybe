    "socket.io": "3.0.4",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-trybe-backend": "1.0.3",
    "nodemon": "2.0.4",
    "jest": "26.0.1",
    "sequelize-cli": "6.2.0",
    "supertest": "4.0.2",
    "shelljs": "0.8.4"
